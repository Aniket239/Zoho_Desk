<!DOCTYPE html>
<html>
<head>
  <title>ZohoDesk</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag asset_path('favicon-16x16.png'), rel: 'icon', type: 'image/png', sizes: '16x16' %>
  <%= favicon_link_tag asset_path('favicon-32x32.png'), rel: 'icon', type: 'image/png', sizes: '32x32' %>
  <%= favicon_link_tag asset_path('apple-touch-icon.png'), rel: 'apple-touch-icon', sizes: '180x180' %>
  <link rel="manifest" href="<%= asset_path('site.webmanifest') %>">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
  <style>
    *{
      margin:0;
      padding:0;
    }
  </style>
</head>
<body>
  <%= render 'layouts/navbar' %>
  <%= yield %>

</body>
</html>
